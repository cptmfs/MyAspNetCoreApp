@model List<StudentViewModel>

<table class="table table-striped table-hover" style="text-align:center">
    <thead>
        <tr>
            <th>Id</th>
            <th>Adı</th>
            <th>Soyadı</th>
            <th>Doğum Tarihi</th>
            <th>Ünvan</th>
            <th>Deneyimler</th>
            <th>Yetenekler</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.DateOfBirth</td>
                    <td>@item.Title</td>
                    <td>@item.Experiences</td>
                    <td>@item.Skills</td>
                </tr>
        }
    </tbody>
</table>